{
    "name": "richardfullmer/pr-bot",
    "description": "Checks pull requests to ensure tests pass",
    "authors": [
        {
            "name": "Richard Fullmer",
            "email": "richardfullmer@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "version": "dev-master",
                "name": "twitter/bootstrap",
                "source": {
                    "url": "git@github.com:twitter/bootstrap.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "vcs",
            "url": "https://github.com/richardfullmer/php-github-api"
        }
    ],
    "require": {
        "silex/silex": "1.0.x-dev",
        "twig/twig": "*",
        "knplabs/github-api": "dev-status-create-fix as 1.1.x-dev",
        "symfony/process": "*",
        "symfony/yaml": "*",
        "symfony/console": "*",
        "symfony/filesystem": "*",
        "swiftmailer/swiftmailer": "*",
        "imagine/Imagine": "*",
        "twitter/bootstrap": "dev-master"
    },
    "autoload": {
        "psr-0": { "RDF\\": "src/" }
    }
}
